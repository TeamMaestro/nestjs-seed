{
    "name": "nestjs-seed",
    "version": "1.0.0",
    "description": "Maestro NestJS seed project",
    "license": "ISC",
    "author": "TeamHive",
    "contributors": [
        "John Pinkster <jpinkster>"
    ],
    "homepage": "https://github.com/TeamHive/nestjs-seed",
    "repository": {
        "type": "git",
        "url": "https://github.com/TeamHive/nestjs-seed.git"
    },
    "bugs": {
        "url": "https://github.com/TeamHive/nestjs-seed/issues"
    },
    "scripts": {
        "prestart": "npm run compile",
        "compile": "npm run clear && tsc -p .",
        "start": "gulp start",
        "start:debug": "nodemon --inspect-brk=9229 ./dist/server.js",
        "test": "jest --config=jest.json",
        "clear": "rm -rf dist",
        "postversion": "git push --tag",
        "migrations": "db-migrate up --config migrations/config.json",
        "migrations:create": "db-migrate create --config migrations/config.json --sql-file --",
        "docs": "rm -Rf docs && apidoc -i src/ -o docs/ && open docs/index.html",
        "build.docs": "rm -Rf docs && apidoc -i src/ -o docs/",
        "docker.cmd": "run-parts ./scripts && nohup forever dist/server.js",
        "docker.cmd.local": "sh ./migrations/run-migrations.sh && nohup forever -c \"node --inspect=0.0.0.0:5858\" dist/server.js"
    },
    "main": "index.js",
    "dependencies": {
        "@local/keys": "file:./keys",
        "@nestjs/common": "^5.4.1",
        "@nestjs/core": "^5.4.1",
        "@nestjs/microservices": "^5.1.0",
        "@nestjs/passport": "^5.1.0",
        "@nestjs/testing": "^5.4.1",
        "@teamhive/express-http-logger": "0.0.3",
        "@teamhive/honeyflow-express-client": "0.0.3",
        "@teamhive/nestjs-common": "^2.2.1",
        "@teamhive/node-document-service": "^1.0.3",
        "@teamhive/sequelize-common": "^1.0.0",
        "bcrypt": "^3.0.2",
        "body-parser": "^1.18.3",
        "class-validator": "^0.9.1",
        "config": "^3.0.0",
        "cookie-parser": "^1.4.3",
        "db-migrate": "^0.11.1",
        "db-migrate-pg": "^0.4.0",
        "helmet": "^3.12.1",
        "js-yaml": "^3.12.0",
        "jsonwebtoken": "^8.3.0",
        "log4js": "^3.0.6",
        "passport": "^0.4.0",
        "passport-google-oauth": "^1.0.0",
        "passport-jwt": "^4.0.0",
        "pg": "^7.7.1",
        "raven": "^2.6.4",
        "redis": "^2.8.0",
        "reflect-metadata": "^0.1.12",
        "request": "^2.87.0",
        "rxjs": "^6.3.3",
        "sequelize": "^4.41.2",
        "sequelize-typescript": "0.6.6",
        "sqlstring": "^2.3.1"
    },
    "devDependencies": {
        "@types/config": "0.0.34",
        "@types/cookie-parser": "^1.4.1",
        "@types/express": "^4.16.0",
        "@types/helmet": "0.0.38",
        "@types/jest": "^23.3.0",
        "@types/jsonwebtoken": "^7.2.8",
        "@types/lodash": "^4.14.112",
        "@types/node": "^12.0.10",
        "@types/passport": "^0.4.6",
        "@types/passport-google-oauth": "^1.0.39",
        "@types/passport-jwt": "^3.0.1",
        "@types/pg": "^7.4.10",
        "@types/raven": "^2.5.1",
        "@types/redis": "^2.8.6",
        "@types/sequelize": "^4.27.24",
        "@types/sqlstring": "^2.2.0",
        "apidoc": "^0.17.6",
        "gulp": "^3.0.0",
        "gulp-nodemon": "^2.4.1",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^5.0.0",
        "jest": "^23.4.1",
        "nodemon": "^1.18.3",
        "ts-jest": "^23.0.1",
        "ts-node": "^7.0.0",
        "tslint": "^5.11.0",
        "typescript": "^3.1.6"
    }
}
